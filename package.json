  "name": "neo4j-record-converter",
  "author": "seihmd",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.5",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.1.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "neo4j-driver": "^4.4.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
